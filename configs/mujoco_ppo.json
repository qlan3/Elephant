{
    "task": [{"name": ["HalfCheetah-v4", "Hopper-v4", "Walker2d-v4", "Ant-v4", "Reacher-v4", "Swimmer-v4"]}],
    "agent": [{
        "name": ["PPO"],
        "model_name": ["PPONet"],
        "model_cfg": [
            {
                "hidden_dims": [[1000]],
                "hidden_act": ["Tanh"]
            },
            {
                "hidden_dims": [[1000]],
                "hidden_act": ["Elephant"],
                "d": [4],
                "h": [1],
                "sigma": [0.8],
                "bias_std": [1.0]
            },
            {
                "hidden_dims": [[150]],
                "hidden_act": ["FTA"],
                "fta_k": [20],
                "fta_bound": [20.0]
            },
            {
                "hidden_dims": [[1000]],
                "hidden_act": ["Maxout"],
                "maxout_k": [5]
            },
            {
                "hidden_dims": [[1000]],
                "hidden_act": ["LWTA"],
                "lwta_k": [5]
            }
        ],
        "collect_steps": [2048],
